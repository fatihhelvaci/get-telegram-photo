{
  "github": {
    "autoAlias": true,
    "enabled": true
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "https://github.com/surabayajs/get-telegram-photo"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "public": true
}
